   "source": [
    "# 정리\n",
    "conn = sqlite3.connect('btc_price.db')\n",
    "cur = conn.cursor()\n",
    "\n",
    "cur.execute('delete from btc_price')\n",
    "\n",
    "df = cur.fetchall()\n",
    "conn.commit()\n",
    "conn.close()"
   ]
