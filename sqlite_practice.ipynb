   "source": [
    "# 잘 추가 되는지 확인\n",
    "add_data()\n",
    "\n",
    "conn = sqlite3.connect('btc_price.db')\n",
    "cur = conn.cursor()\n",
    "# 쿼리입력\n",
    "cur.execute('select * from btc_price')\n",
    "# 결과 df에 할당\n",
    "df = cur.fetchall()\n",
    "conn.close()\n",
    "\n",
    "# 컬럼명 cols에 할당\n",
    "cols = [i[0] for i in cur.description]\n",
    "\n",
    "# 데이터프레임화\n",
    "df = pd.DataFrame(df, columns = cols)\n",
    "df = df.set_index('date')\n",
    "\n",
    "# 미리보기\n",
    "print(df.shape)\n",
    "df.head()"
   ]
