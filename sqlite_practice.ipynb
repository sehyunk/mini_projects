   ],
   "source": [
    "# 전일의 종가, btc_price_close에 할당\n",
    "btc_price_close = btc_price.iloc[0,3]\n",
    "\n",
    "# 전일 날짜 yesterday에 할당\n",
    "yesterday = datetime.today() - timedelta(1)\n",
    "yesterday = yesterday.strftime('%Y-%m-%d')\n",
    "\n",
    "# 튜플로 묶어 data에 할당\n",
    "data = (yesterday, btc_price_close)\n",
    "data"
   ]
  },
