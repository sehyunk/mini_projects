   "source": [
    "# 최근 2일의 비트코인 데이터 불러오기\n",
    "btc_price = pyupbit.get_ohlcv(\"KRW-BTC\", count = 2)\n",
    "btc_price"
   ]
  },
  {
   "cell_type": "code",
